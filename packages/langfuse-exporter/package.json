{
  "name": "@voltagent/langfuse-exporter",
  "description": "OpenTelemetry SpanExporter for sending VoltAgent traces to Langfuse.",
  "version": "1.1.3",
  "dependencies": {
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "langfuse": "^3.37.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@voltagent/core": "^1.1.30"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0",
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@voltagent/core": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VoltAgent/voltagent.git",
    "directory": "packages/src"
  },
  "scripts": {
    "attw": "attw --pack",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "publint": "publint --strict",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "types": "dist/index.d.ts"
}
