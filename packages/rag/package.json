{
  "name": "@voltagent/rag",
  "description": "Chunking and RAG utilities for VoltAgent",
  "version": "0.0.1",
  "dependencies": {
    "js-tiktoken": "^1.0.14"
  },
  "devDependencies": {
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/VoltAgent/voltagent.git",
    "directory": "packages/rag"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --reporter=hanging-process --reporter=default --typecheck",
    "test:coverage": "vitest run --coverage --typecheck"
  },
  "types": "dist/index.d.ts"
}
