{
  "name": "@voltagent/libsql",
  "description": "VoltAgent LibSQL - LibSQL/Turso Memory provider integration for VoltAgent",
  "version": "2.1.1",
  "dependencies": {
    "@libsql/client": "^0.15.0",
    "@voltagent/internal": "^1.0.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@voltagent/core": "^2.3.7",
    "@voltagent/logger": "^2.0.2",
    "ai": "^6.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./edge": {
      "import": {
        "types": "./dist/edge.d.mts",
        "default": "./dist/edge.mjs"
      },
      "require": {
        "types": "./dist/edge.d.ts",
        "default": "./dist/edge.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "peerDependencies": {
    "@voltagent/core": "^2.0.0",
    "@voltagent/logger": "^2.0.0",
    "ai": "^6.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VoltAgent/voltagent.git",
    "directory": "packages/libsql"
  },
  "scripts": {
    "attw": "attw --pack",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "publint": "publint --strict",
    "test": "vitest --reporter=hanging-process --reporter=default --typecheck",
    "test:coverage": "vitest run --coverage --typecheck"
  },
  "types": "dist/index.d.ts"
}
